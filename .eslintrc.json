{
  "extends": ["eslint:recommended", "plugin:react/recommended", "next"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "react/prop-types": 0
  }
}
